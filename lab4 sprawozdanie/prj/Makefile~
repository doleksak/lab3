__start__: program
	./program

program: quick.o time.o main.o heap.o merge.o
	c++ main.o quick.o time.o heap.o merge.o -o program

quick.o: quick.cpp
	c++ -c quick.cpp -o quick.o

time.o: time.cpp
	c++ -c time.cpp -o time.o

main.o: main.cpp
	c++ -c main.cpp -o main.o

heap.o: heap.cpp
	c++ -c heap.cpp -o heap.o

merge.o: merge.cpp
	c++ -c merge.cpp -o merge.o

clean:
	rm -f *.o